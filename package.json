{
    "name": "@aurogpt/flowgpt",
    "license": "MIT",
    "version": "1.0.0",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "repository": {
        "url": "https://github.com/aurogpt/flowgpt"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "scripts": {
        "release": "yarn build && changeset publish",
        "build": "tsup src/*.ts src/**/*.ts --dts --format esm,cjs,mjs --minify --clean --onSuccess 'cp -r src/contents dist/'",
        "lint": "eslint ./src "
    },
    "devDependencies": {
        "@changesets/cli": "^2.27.11",
        "@eslint/js": "^9.17.0",
        "@types/bun": "latest",
        "globals": "^15.14.0",
        "tsup": "^8.3.5",
        "typescript-eslint": "^8.19.0"
    },
    "peerDependencies": {
        "typescript": "^5.7.2"
    },
    "dependencies": {
        "@langchain/core": "^0.3.27",
        "@langchain/openai": "^0.3.16",
        "@types/node": "^22.10.3",
        "@typescript-eslint/eslint-plugin": "^8.19.0",
        "@typescript-eslint/parser": "^8.19.0",
        "eslint": "^9.17.0",
        "langchain": "^0.3.9"
    },
    "publishConfig": {
        "provenance": true,
        "access": "public"
    }
}
